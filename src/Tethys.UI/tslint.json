{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": false,
    "trailing-comma": false,
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-var-requires": false,
    "max-line-length": [
      160
    ],
    "max-file-line-count": [
      250
    ],
    "import-spacing": false,
    "space-before-function-paren": [
      false,
      {
        "anonymous": "always"
      }
    ]
  },
  "rulesDirectory": []
}